{
  "name": "pgl-g-editor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "./node_modules/.bin/ng serve --proxy-config ./proxy.json  --project=pgl-g-editor-tester",
    "start:docker": "npm start -- --proxy-config --progress=true ./proxy.docker.json",
    "docker": "wait-on -l http://server:9000/api && npm run wait:deps && npm run start:docker -- --host 0.0.0.0 --poll 1",
    "start:server": "./node_modules/.bin/ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start:server\"",
    "wait:deps": "wait-on -l dist/pgl-g-editor/pgl-g-editor.d.ts",
    "start:dev": "rimraf dist && npm run build:watch && npm run wait:deps && npm start",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "test": "ng test",
    "lint": "ng lint",
    "lint:eslint": "./node_modules/.bin/eslint './**/*.ts' --ignore-pattern='*.spec.ts'",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "@wordpress/block-serialization-spec-parser": "^3.5.0",
    "@wordpress/edit-post": "^3.16.0",
    "@wordpress/format-library": "^1.17.0",
    "@wordpress/list-reusable-blocks": "^1.16.0",
    "@wordpress/nux": "^3.15.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "oembed-providers": "^1.0.20170414",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular-devkit/build-ng-packagr": "~0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.150",
    "@types/multer": "^1.4.3",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "codelyzer": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^24.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "latest": "^0.2.0",
    "multer": "^1.4.2",
    "ng-packagr": "^10.1.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "~5.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.0"
  }
}
